# Worker runtime
APP_ENV=production
NODE_ENV=production

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Internal API
API_INTERNAL_URL=http://api:3000
WORKER_INTERNAL_TOKEN=CHANGE_ME_WORKER_INTERNAL_TOKEN

# Object storage (production: external S3 provider, e.g. Beget S3)
S3_ENDPOINT=CHANGE_ME_S3_ENDPOINT
S3_REGION=us-east-1
S3_BUCKET=continuum
S3_ACCESS_KEY_ID=CHANGE_ME_S3_ACCESS_KEY_ID
S3_SECRET_ACCESS_KEY=CHANGE_ME_S3_SECRET_ACCESS_KEY
S3_FORCE_PATH_STYLE=false
S3_PUBLIC_BASE_URL=CHANGE_ME_S3_PUBLIC_BASE_URL

# LaTeX queue
LATEX_COMPILE_TIMEOUT_MS=120000
LATEX_COMPILE_WORKER_CONCURRENCY=1
