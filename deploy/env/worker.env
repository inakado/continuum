# Worker runtime
APP_ENV=production
NODE_ENV=production

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Internal API
API_INTERNAL_URL=http://api:3000
WORKER_INTERNAL_TOKEN=CHANGE_ME_WORKER_INTERNAL_TOKEN

# Object storage
S3_ENDPOINT=http://minio:9000
S3_REGION=us-east-1
S3_BUCKET=continuum
S3_ACCESS_KEY_ID=continuum-minio
S3_SECRET_ACCESS_KEY=CHANGE_ME_MINIO_PASSWORD
S3_FORCE_PATH_STYLE=true
S3_PUBLIC_BASE_URL=https://files.example.com

# LaTeX queue
LATEX_COMPILE_TIMEOUT_MS=120000
LATEX_COMPILE_WORKER_CONCURRENCY=1
