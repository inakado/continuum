{
  "name": "@continuum/api",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "postinstall": "prisma generate",
    "build": "node ../../scripts/ensure-docker-build.cjs && pnpm exec tsc -p tsconfig.json",
    "typecheck": "node ../../scripts/ensure-docker-build.cjs && pnpm exec tsc -p tsconfig.json --noEmit",
    "test": "node -e \"console.log('No tests for @continuum/api yet')\"",
    "start": "node dist/main.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.988.0",
    "@aws-sdk/lib-storage": "^3.988.0",
    "@aws-sdk/s3-request-presigner": "^3.988.0",
    "@continuum/shared": "workspace:*",
    "@nestjs/common": "^11.1.12",
    "@nestjs/core": "^11.1.12",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-express": "^11.1.12",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.0.0",
    "@smithy/node-http-handler": "^4.4.10",
    "argon2": "^0.41.0",
    "bullmq": "^5.67.2",
    "cookie-parser": "^1.4.7",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.18.0",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.10",
    "dotenv": "^16.4.7",
    "prisma": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
